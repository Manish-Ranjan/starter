{
    "name": "template",
    "version": "0.0.0",
    "description": "",
    "private": true,
    "main": "dist/cjs/index.js",
    "module": "dist/es/index.js",
    "types": "dist/es/index.d.ts",
    "files": ["dist"],
    "keywords": [],
    "author": "Medly",
    "license": "ISC",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/medly"
    },
    "scripts": {
        "build": "rollup --config node_modules/@medly/rollup-config/index.js",
        "commit": "git-cz",
        "lint": "eslint 'src/**/*.ts'",
        "release": "semantic-release",
        "start": "ts-node ./src/index.ts",
        "pretest": "yarn typecheck",
        "test": "jest --passWithNoTests --coverage -w=50%",
        "typecheck": "tsc -p tsconfig.json"
    },
    "dependencies": [],
    "devDependencies": [
        "@commitlint/cli",
        "@commitlint/config-conventional",
        "@medly/babel-config",
        "@medly/eslint-config",
        "@medly/prettier-config",
        "@medly/rollup-config",
        "@medly/semantic-release-config",
        "@medly/typescript-config",
        "commitizen",
        "cross-env",
        "cz-conventional-changelog",
        "husky@4.0.0",
        "jest",
        "ts-node"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "commitlint": {
        "extends": ["@commitlint/config-conventional"]
    },
    "husky": {
        "hooks": {
            "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "release": {
        "extends": "@medly/semantic-release-config"
    },
    "babel": {
        "extends": "@medly/babel-config"
    },
    "prettier": "@medly/prettier-config",
    "eslintConfig": {
        "extends": "@medly"
    }
}
